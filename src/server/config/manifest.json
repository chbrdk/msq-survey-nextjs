      "strict": true,
      "values": ["MSQ", "UDG", "MMT", "26DX", "SPACESHP", "THE GATE"]
    },
    "departments": {
      "type": "enum",
      "strict": false,
      "suggested_values": [
        "Creative & Design",
        "Content & Strategy",
        "Account Management",
        "Paid Media & Performance",
        "SEO & Organic Growth",
        "Analytics & Insights",
        "Project/Delivery Management",
        "Business Development",
        "Operations & Admin",
        "Finance",
        "HR & People",
        "Technology & Development",
        "UX/UI Design",
        "Engineering/Development"
      ]
    },
    "job_levels": {
